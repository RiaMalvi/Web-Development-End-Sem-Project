.menu-items {z-index:2;
  
  list-style: none;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.menu-items li a {z-index:2;
  font-size: 1rem;
  padding: 0.5em 1em;
  transition: background 0.3s ease-in-out, color 0.3s ease-in-out;
}

.menu-items a:hover {z-index:2;
  color: black;
  font-size: 21px;
}

/* nav menu button */
.menu-btn {
  position: relative;
  display: none;
  justify-content: center;
  align-items: center;
  width: 1.5rem;
  height: 1.5rem;
  cursor: pointer;
  z-index:3;
}
.menu-btn__lines,
.menu-btn__lines::before,
.menu-btn__lines::after {z-index:3;
  width: 1.5rem;
  height: 0.1rem;
  background-color: #111111;
  transition: all 0.5s ease-in-out;
}
.menu-btn__lines::before,
.menu-btn__lines::after {z-index:3;
  content: "";
  position: absolute;
}
.menu-btn__lines::before {z-index:3;
  transform: translateY(-0.5rem);
}
.menu-btn__lines::after {z-index:3;
  transform: translateY(0.5rem);
}
/* animation */
.menu-btn.open .menu-btn__lines {z-index:3;
  transform: translateX(2rem);
  background-color: transparent;
}
.menu-btn.open .menu-btn__lines::before {z-index:3;
  transform: rotate(45deg) translate(-1.5rem, 1.5rem);
  background-color: #ffffff;
}
.menu-btn.open .menu-btn__lines::after {z-index:3;
  transform: rotate(-45deg) translate(-1.5rem, -1.5rem);
  background-color: #ffffff;
}

@media (max-width: 925px) {
